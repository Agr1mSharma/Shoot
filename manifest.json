{
    "manifest_version": 3,
    "name": "Subway Surfer Extension",
    "version": "1.0",
    "description": "Play local video in a resizable popup window",
    "permissions": [
      "windows"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }